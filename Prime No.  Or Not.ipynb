   "source": [
    "def isPrime(n):\n",
    "    if n<=1:\n",
    "        return False\n",
    "    \n",
    "    for i in range(2,n):\n",
    "        if n%i==0:\n",
    "            return False\n",
    "        \n",
    "    return True\n",
    "\n",
    "n = int(input())\n",
    "if isPrime(n):\n",
    "    print(\"{} is a Prime Number\".format(n))\n",
    "else:\n",
    "    print(\"{} is not a Prime Number\".format(n))"
